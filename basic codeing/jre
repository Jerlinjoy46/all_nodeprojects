#include <stdio.h>
int main()
{
int i,prime,flag=0;
    printf("enter a number:");
    scanf("%d",&prime);
    for(i=2;i<prime/2;i++){
        if(prime%i==0){
            flag=1;
            break;
        }
    }
    if(flag==0){
        printf("number is prime%d",prime);
    }else{
        printf("number is not a prime %d",prime);
    }
} 
